package ex01변수;

public class Ex02기본변수 {
	
	
	public static void main(String[] args) {
		// Java의 자료형
		// 1. 기본타입 (Primitive, Basic)
		
		// 정수타입 (byte, short, int, long)
		// byte - 8bit, 1byte, 256 (-128 ~ 127)
		byte  num1 = 127;
		
		// short - 2byte, 16bit, 
		short  num2 = 11111;
		
		
		// int - 4byte, 32bit
		int  num3 = 12345678;
		
		// long - 8byte, 64bit
		long  num4 = (long)3000000000;
		
		
		
		
		// 2. 참조타입 (Reference) - 추후
	}

}
