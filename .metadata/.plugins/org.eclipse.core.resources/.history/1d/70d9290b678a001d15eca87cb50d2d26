package ex02연산자;

public class Ex02산술연산자실습 {

	public static void main(String[] args) {
		// 문제. 백의 자리 이하 숫자를 모두 버리세요.
		int iNum = 556;
		/////////////////////////////////////////////////////////////
		// 결과확인 : 400
		// 단 이건 안됩니다.
		// 1. num = 400;
		// 2. 출력문에 400을 쓰는 경우.
		/////////////////////////////////////////////////////////////
		
		int iHundread;
		
		iHundread = iNum / 100;
		System.out.println("iHundread : " + iHundread);
		
		iNum = iHundread * 100;
		/////////////////////////////////////////////////////////////
		System.out.println("결과확인 : " + iNum);

	}

}
